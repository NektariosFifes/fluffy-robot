{
  "title": "Simulation Plot 0.0.1 schema",
  "$id": "simulator_id",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "required": [
    "plotVer",
    "info",
    "Plots"
  ]
,
  "additionalProperties": false,
  "type": "object",
  "properties": {
    "plotVer": {
      "type": "string"
    },
    "info": {
      "type": "string"
    },
    "Plots": {
      "type": "object",
      "additionalProperties": {
        "type": "object",
        "additionalProperties": false,
        "patternProperties": {
          "^plot-[\\d\\w]{1,14}$": {
            "type": "object",
            "additionalProperties": false,
            "required": [
              "channels"
            ],

            "properties": {
              "channels": {
                "type": "array",
                "items": {
                  "type": "string",
                  "format": "uri-template",
                  "minLength": 1
                },
                "uniqueItems": true,
                "minItems": 1,
                "maxItems": 10
              },
              "NoOfRequests" : {
                "type": "integer"
              },
              "PercentageOfLoad" : {
                "type": "integer",
                "minimum": 0,
                "maximum": 100


              }


            }
          }
        },
        "properties": {

        }
        }
      }

  },
  "parameters": {
    "type": "object",
    "additionalProperties": {
      "$ref": "#/"
    }
  }

}